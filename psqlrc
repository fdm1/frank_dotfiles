-- \set PROMPT1 '%[%033[36m%]%n@%/\n%R%x%#%[%033[37m%] '
\set PROMPT1 '%[%033[36m%]%`date` %[%033[32m%]%n %[%033[31m%]localhost%[%033[0m%] %/%[%033[36m%]\n%R%x%#%[%033[37m%] '

-- '... -'
\set PROMPT2 '%[%033[32m%]... %[%033[36m%]%R%x%[%033[37m%] '


